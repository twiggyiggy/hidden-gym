 <section class="welcome-title">
     <div>
    <h2>Welcome, {{currentUser.username}}!</h2>
     </div>
     <div style="flex-shrink: 0">
    <h3>HIDDEN GYMS IN BARCELONA- </h3>

     </div>
 </section>


<section class="gyms-list-gallery">
    {{#each gyms}}
    <a class="this-gym-details" href="/gyms/{{_id}}/details">
    <h3>{{address}}</h3>
        <ul>
            <li>
                <img src="{{image}}">
            </li>

            </li>
                {{#if averageRating}}
                    <p>GYM RATING: {{averageRating}}%</p>
                {{else}}
                    <p>GYM RATING: No votes yet. Be the first one to rate it!</p>
                {{/if}}
            </li>

        </ul>
    </a>
    {{/each}}
</section>